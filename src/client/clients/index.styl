.Clients
  text-align center
  margin-bottom 3em
  padding 1em

  &-title
    font-size 2em
    font-weight normal
    color gray
    border-bottom: 1px solid gray
    padding-bottom .5em
  &-logos
    padding 0
    margin 0
    display block
  &-logoContainer
    list-style none
    width 120px
    height 90px
    display inline-block
    margin-right 1em
  &-logo
    max-width 100%
    max-height: 90px
    height auto
    display inline-block
    vertical-align: middle
